{
  "name": "my-rescript-vite-app",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-webapi",
    "@glennsl/rescript-fetch",
    "@glennsl/rescript-json-combinators"
  ],
  "bs-dev-dependencies": [
  ],
  "bsc-flags": [
    "-open RescriptCore",
    "-open JsonCombinators"
  ]
}
